{
  "name": "wallpapers",
  "version": "1.0.0",
  "description": "<div align=\"center\">   <h1>Wallpaper Gallery</h1>   <p>A curated collection of stunning wallpapers, ready for one-click deployment.</p> </div>",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "pnpm run build:assets && esbuild docs/js/app.js --bundle --outfile=public/js/bundle.js --loader:.css=css --allow-overwrite --servedir=public --serve=0.0.0.0:8000",
    "build": "pnpm run build:assets && esbuild docs/js/app.js --bundle --outfile=public/js/bundle.js --loader:.css=css --allow-overwrite --minify",
    "start": "pnpm run build && http-server public --host 0.0.0.0 --port 8000",
    "build:assets": "rm -rf public && mkdir -p public && cp -r docs/* public/ && cp -r src public/",
    "lint": "eslint docs/js/**/*.js --fix",
    "format": "prettier --write docs/js/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rishabh5321/wallpapers.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rishabh5321/wallpapers/issues"
  },
  "homepage": "https://github.com/Rishabh5321/wallpapers#readme",
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "basiclightbox": "^5.0.4",
    "http-server": "^14.1.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.6",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-standard": "^17.1.0",
    "prettier": "^3.6.2"
  }
}
